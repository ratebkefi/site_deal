{% extends '::baseBack.html.twig' %}
{% form_theme form 'form_table_layout.html.twig' %}
{% block body -%}
    <div class="content">
        <div class="container-fluid">
            <!-- ==================== COMMON ELEMENTS ROW ==================== -->
            <div class="row-fluid">
                <div class="span12">
                    <!-- ==================== TEXT INPUTS HEADLINE ==================== -->
                    <div class="containerHeadline">
                        <i class="icon-edit"></i>
                        {% if id is defined %}
                            <h2>Modifier un point de vente</h2>
                        {% else %}
                            <h2>Ajouter un point de vente</h2>
                        {% endif %}
                        <div class="controlButton pull-right"><i class="icon-remove removeElement"></i></div>
                        <div class="controlButton pull-right"><i class="icon-caret-down minimizeElement"></i></div>
                    </div>
                    <!-- ==================== END OF TEXT INPUTS HEADLINE ==================== -->
                    <!-- ==================== TEXT INPUTS FLOATING BOX ==================== -->
                    <div class="floatingBox">
                        <div class="container-fluid">
                            <h2>Point de vente de partenaire : {{ partner.name }}</h2>

                            <div class="container-fluid">
                                {% if id is defined %}
                                <form class="form-horizontal contentForm" method="POST"
                                      action="{{ path('dash_sellingpoint_update',{'id' : id,'partid' : partid}) }}">
                                    {% else %}
                                    <form class="form-horizontal contentForm" method="POST"
                                          action="{{ path('dash_sellingpoint_add',{'partid' : partid}) }}">
                                        {% endif %}
                                        <div class="span4">
                                            {{ form_row(form.name, {'attr': {'onchange':'this.value = this.value.charAt(0).toUpperCase() + this.value.substr(1);'}}) }}
                                            {{ form_row(form.adress) }}
                                            <div class="control-group">
                                                <label for="gouvernorat" class="control-label">Gouvernorat</label> 
                                                <div class="controls">
                                                    <select name="gouvernorat" onChange="javascript:sendData(''+this.value+'')">
                                                        {% for item in gouvernorat %}
                                                        <option value="{{ item.id }}" {% if id is defined %}
							{% if item.id ==  selling.localite.parent.parent.id and ville != null %} selected {% endif %} {% endif %}>{{ item.name }}</option>
                                                        {% endfor %}

                                                    </select>
                                                </div>
                                            </div>
                                            <div class="control-group" id="delegation" >
                                                <label for="gouvernorat" class="control-label">Délegations</label>
                                                <div class="controls" id="delegation1">
                                                        <select name="delegation" required="required" id="delegation2" onChange="javascript:sendData2(''+this.value+'')">
														{% if ville != null %}<option value="{{ selling.localite.parent.id }}" selected>{{ selling.localite.parent }}</option>{% endif %}			
                                                        </select>
                                                </div>
                                            </div>
                                            <div class="control-group" id="localite" >
                                                <label for="gouvernorat" class="control-label">Ville</label>
                                                <div class="controls" id="delegation4">
                                                    <select name="ville" id="delegation3"  required="required" onChange="javascript:afficherCodePostale(''+this.value+'')">
                                                        {% if ville != null %}<option value="{{ selling.localite.id }}" selected>{{ ville }}</option>{% endif %}
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="control-group ">
                                                <label for="back_partnerbundle_sellingpoint_nbrEmployee" class="control-label">Code postale</label>
                                                <div class="controls">
                                                    <input type="text" name="cpadd" id="cpadd" value="{{ cp }}" required="required" readonly="readonly" >
                                                </div>
                                            </div>
                                            {{ form_row(form.latitude) }}
                                            {{ form_row(form.longitude) }}
                                            {{ form_row(form.phone) }}
                                            {{ form_row(form.visible) }}
                                            {% set tsst=0 %}
                                            {% if selling is defined %}
                                                {% set tsst=selling.visible %}
                                            {% endif %}
                                            <div id="showvisible"
                                                 style="display: {% if tsst==1 %}block{% else %}none{% endif %}">
                                                {{ form_row(form.visibleAdress) }}
                                                {{ form_row(form.latVisibleAdress) }}
                                                {{ form_row(form.lonVisibleAdress) }}
                                            </div>
                                            {{ form_row(form.nbrEmployee) }}
                                            {{ form_row(form.capacityPerHour) }}

