                                                                    {% set endm = arrday['Lundi']['CloseTimeMorning'] %}
                                                                    {% set starta = arrday['Lundi']['OpenTimeAfternoon'] %}
                                                                    {% set enda = arrday['Lundi']['CloseTimeAfternoon'] %}
