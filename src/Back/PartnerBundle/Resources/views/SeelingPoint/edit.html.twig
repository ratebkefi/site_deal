     <script>
         function loadallvalue(div, val) {

            // $("." + div).val(val);
         }
         $(document).ready(function () {
             $("#back_partnerbundle_sellingpoint_name").attr({
                 'data-validation': 'length',
                 'data-validation-length': 'min4'
             });

             $("#back_partnerbundle_sellingpoint_latitude").attr({
                 'data-validation': 'number',
                 'data-validation-allowing': 'float'
             });
             $("#back_partnerbundle_sellingpoint_longitude").attr({
                 'data-validation': 'number',
                 'data-validation-allowing': 'float'
             });
             $("#back_partnerbundle_sellingpoint_phone").attr({
                 'data-validation': 'number',
                 'data-validation': 'length number', 'data-validation-length': '8-8'
             });

             $("#back_partnerbundle_sellingpoint_nbrEmployee").attr({
                 'data-validation': 'number',
                 'data-validation-allowing': 'float,positive'
             });
             $("#back_partnerbundle_sellingpoint_capacityPerHour").attr({
                 'data-validation': 'number',
                 'data-validation-allowing': 'float,positive'
             });
             $.validate();
