                                                        <tr>
                                                            <th nowrap>Matin 1</th>
                                                            <th nowrap>Matin 2</th>
                                                            <th nowrap>Aprés-midi 1</th>
                                                            <th nowrap>Aprés-midi 2</th>
                                                            <th nowrap>Activé?</th>
                                                        </tr>
                                                        </thead>
                                                        <tbody>
                                                        <tr>
                                                            {% set startm = '08:00' %}
                                                            {% set endm = '12:00' %}
                                                            {% set starta = '14:00' %}
                                                            {% set enda = '18:00' %}
                                                            {% set tstexist = 1 %}
