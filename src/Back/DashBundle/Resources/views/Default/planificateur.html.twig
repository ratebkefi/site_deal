{% extends '::baseBack.html.twig' %}
{% block body %}
<div class="container-fluid">
    <div class="row-fluid" style="margin-bottom:0">
        <div class="span12">
            <div class="row-fluid" style="margin-bottom:0">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-zoom-in"></i>
                        <h2>Filtre</h2>
                    </div>
                    <div class="floatingBox">
                        <div class="container-fluid">
                            <form onsubmit="return false;">

                                <div class="span4">
                                    <label>Date de début</label>
                                    <input type="text" class="span10 datetime" name="dated" id="dated"
                                           value="{{ dated|date('d/m/Y') }}"/>
                                </div>
                                <div class="span4">
                                    <label>Date de fin</label>
                                    <input type="text" name="datef" class="span10 datetime" id="datef"
                                           value="{{ datef|date('d/m/Y') }}"/>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>Nombre d’annexes choisis par commercial </h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadAnnexeCommercialChart()"></i></div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <div class="span6">
                                    <form class="form-horizontal contentForm">
                                        <div class="control-group">
                                            <label class="control-label" for="commercial"
                                                   style="width: 70px !important;">Commercial</label>

                                            <div class="controls" style="margin-left: 86px !important;">
                                                <select id="commercial1" name="commercial1" class=" "
                                                        onchange="loadAnnexeCommercialChart()">
                                                    <option value="">Choisissez un commercial</option>
                                                    {% for item in commercial %}
                                                        <option value="{{ item.id }}">{{ item.name }}</option>

                                                    {% endfor %}

                                                </select>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <br><br><br><br>

                                <div id="annexecommercial" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>Nombre de contrats signés par commercial</h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadNombreContratParcommercialChart()"></i>
                        </div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <div class="span6">
                                    <form class="form-horizontal contentForm">
                                        <div class="control-group">
                                            <label class="control-label" for="commercial"
                                                   style="width: 70px !important;">Commercial</label>

                                            <div class="controls" style="margin-left: 86px !important;">
                                                <select id="commercial" name="commercial" class=" "
                                                        onchange="loadNombreContratParcommercialChart()">
                                                    <option value="">Choisissez un commercial</option>
                                                    {% for item in commercial %}
                                                        <option value="{{ item.id }}">{{ item.name }}</option>

                                                    {% endfor %}

                                                </select>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <br><br><br><br>

                                <div id="nombrecontratparcommercial" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!-- ==================== ROW ==================== -->
    <div class="row-fluid">
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>% de deals annulés</h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadDealAnnulerChart()"></i></div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <div id="dealannuler" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">
                                <table id="dealannulerPieChartTable" class="table">
                                    <thead>
                                    <tr>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                    </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>% des nouveaux contrats</h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadNouveauContratChart()"></i></div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <div id="nouveaucontrat" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">
                                <table id="nouveaucontratPieChartTable" class="table">
                                    <thead>
                                    <tr>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                    </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="row-fluid">
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>Nombre de deals par état</h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadNombreDealChart()"></i></div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <form class="form-horizontal contentForm">

                                    <div class="span6">
                                        <label style="width: 96px !important;" for="etat" class="control-label">Etat
                                            planning</label>

                                        <div class="controls" style="margin-left: 106px !important;">
                                            <select id="etat" name="etat" class="span10 "
                                                    onchange="loadNombreDealChart()">
                                                <option value="">Choisissez un état</option>
                                                <option value="0">Pré-Planifié</option>
                                                <option value="1">Planifié</option>
                                                <option value="2">Rédigé</option>
                                                <option value="3">Validé</option>
                                                <option value="4">Annulé</option>
                                            </select>
                                        </div>
                                    </div>
                                </form>
                                <br><br><br><br>

                                <div id="barChart" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">
                                <table id="nombreCommandePieChartTable" class="table">
                                    <thead>
                                    <tr>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                    </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="row-fluid">
                <div class="span12">
                    <div class="containerHeadline">
                        <i class="icon-ticket"></i>

                        <h2>Délai avant la publication des deals</h2>

                        <div class="controlButton pull-right"><i class="icon-refresh"
                                                                 onclick="loadChartPublication()"></i></div>
                    </div>

                    <div class="floatingBox">

                        <div class="container-fluid">
                            <div class="span12">
                                <div id="publicationChart" class="demoChart"></div>
                            </div>
                        </div>
                        <br><br>

                        <div class="container-fluid">
                            <div class="span12">
                                <table id="publicationPieChartTable" class="table">
                                    <thead>
                                    <tr>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                    </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    {% endblock %}
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('RessourcesBack/css/bootstrap-select.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('RessourcesBack/css/jquery.dataTables.css') }}">

    {% endblock %}
    {% block javascripts %}

    <script src="{{ asset('RessourcesBack/js/lib/jquery-inputmask/jquery.inputmask.min.js') }}"></script>
    <script src="{{ asset('RessourcesBack/js/lib/jquery-inputmask/jquery.inputmask.extensions.js') }}"></script>
    <script src="{{ asset('RessourcesBack/js/lib/jquery-inputmask/jquery.inputmask.date.extensions.js') }}"></script>
    <script src="{{ asset('RessourcesBack/js/vendor/jquery.sparkline.min.js') }}"></script>
    <!-- small charts plugin -->
    <script src="{{ asset('RessourcesBack/js/vendor/jquery.flot.min.js') }}"></script>
    <!-- charts plugin -->
    <script src="{{ asset('RessourcesBack/js/vendor/jquery.flot.resize.min.js') }}"></script>
    <!-- charts plugin / resizing extension -->
    <script src="{{ asset('RessourcesBack/js/vendor/jquery.flot.pie.min.js') }}"></script>
    <!-- charts plugin / pie chart extension -->
    <script src="{{ asset('RessourcesBack/js/bootstrap-select.js') }}"></script>
    {% javascripts '@BackDashBundle/Resources/public/js/jquery.dataTables.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <script type="text/javascript">
        var linkajx12 = "{{ path('back_ajx_ticket12_json') }}";
        var linkajx13 = "{{ path('back_ajx_ticket13_json') }}";
        var linkajx14 = "{{ path('back_ajx_ticket14_json') }}";
        var linkajx15 = "{{ path('back_ajx_ticket15_json') }}";
        var linkajx16 = "{{ path('back_ajx_ticket16_json') }}";
        var linkajx17 = "{{ path('back_ajx_ticket17_json') }}";

    </script>
    {% javascripts '@BackDashBundle/Resources/public/js/planificateur.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}